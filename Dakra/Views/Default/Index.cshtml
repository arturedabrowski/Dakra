<!DOCTYPE html>
@model List<KRD.RestApi.TeamCity.Entity.Project>
  <html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>
  <body>


    @for(int i = 1; i < Model.Count; i++)
    {
    <div class="col-lg-4">
      <label class="control-label">
        @Html.ActionLink(Model[i].Name, "ProjectParameters", "Default", new { @projectId = Model[i].Id }, null)
      </label>
    </div>
    }

  </body>
</html>
